   "execution_count": 16,
   "id": "1e1914e6-ad8c-4537-9876-1fae99fb12d2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Chia d·ªØ li·ªáu: 80% Train, 20% Test\n",
    "(train_df, test_df) = model_df.randomSplit([0.8, 0.2], seed=42)"
