   "source": [
    "# ƒê√°nh gi√° m√¥ h√¨nh"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "936227c8-03d8-4e92-aff8-65f15b0870a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# D·ª± ƒëo√°n tr√™n t·∫≠p Test\n",
    "predictions = lr_model.transform(test_df)"
